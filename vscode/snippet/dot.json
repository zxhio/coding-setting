{
	// Place your snippets for dot here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	"Print to console": {
		"prefix": "dot",
		"body": [
			"digraph $1 {",
				"\tcompound=true",
				"\tranksep = .75",
				"\tnodesep = 1",
				"\tpad = .5",
				"\trankdir = LR",
				"",
				"\tnode [shape = Mrecord, charset = \"UTF-8\" fontname = \"Microsoft YaHei\", fontsize=14]",
				"\tedge [style = solided, charset = \"UTF-8\" fontname = \"Microsoft YaHei\", fontsize=11]",
			"}"
		],
		"description": "Log output to console"
	}
}